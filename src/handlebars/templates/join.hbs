                    <tr>
                        <th class="th">우편번호</th>
                        <td>dd</td>
                    </tr>
                    <tr>
                        <th class="th">주소</th>
                        <td>
                            <div class="input__group vertical address">
                                <div class="input__wrap">
                                    <input type="text" class="input" placeholder="우편번호 주소">
                                </div>
                                <div class="input__wrap">
                                    <input type="text" class="input" placeholder="상세 주소 입력">
                                </div>
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>

        <div class="buttons">
            <button type="button" class="button btnFill">등록</button>
            <button type="button" class="button btnWhite">취소</button>
        </div>
